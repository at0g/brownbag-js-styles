{
  "lerna": "2.2.0",
  "packages": [
    "packages/*"
  ],
  "version": "0.0.1-alpha.0",
  "npmClient": "npm",
  "command": {
    "bootstrap": {
      "hoist": true
    },
    "publish": {
      "allowBranch": "master",
      "forcePublish": "*",
      "cdVersion": "prepatch",
      "preid": "development",
      "skipNpm": true,
      "skipGit": true
    }
  }
}
